{
    "dataset_id": "dataset_content_facebook",
    "start_date": "2020-07-22",
    "end_date": "2020-07-28",
    "filters": [
        {
            "field": "lfm.brand_view.type",
            "operator": "=",
            "values": [
                "CUSTOM"
            ]
        },
        {
            "field": "lfm.brand_view.set_names",
            "operator": "IN",
            "values": [
                "My Brands"
            ]
        }
    ],
    "metrics": [
        "facebook.post.likes"
    ],
    "group_by": [
        "lfm.content.posted_on_date_str",
        "lfm.content.id",
        "lfm.content.channel"
    ],
    "meta_dimensions": [
        "lfm.content.link"
    ],
    "sort": [
        {
            "field": "facebook.post.likes",
            "dir": "DESC"
        }
    ],
    "per_page": 10
}